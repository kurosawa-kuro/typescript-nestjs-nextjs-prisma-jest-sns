{
  "name": "typescript-nestjs-prisma-jest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --config ./__test__/unit/jest-unit.json",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config ./test/unit/jest-unit.json",
    "test:cov": "cross-env NODE_ENV=test jest --config ./__test__/unit/jest-unit.json --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/unit/jest-unit.json",
    "test:e2e": "cross-env NODE_ENV=test dotenv -e .env.test -- jest --maxWorkers=1 --config ./__test__/e2e/jest-e2e.json",
    "prisma:generate:dev": "cross-env NODE_ENV=development dotenv -e .env.development -- prisma generate",
    "prisma:migrate:dev": "cross-env NODE_ENV=development dotenv -e .env.development -- prisma migrate dev",
    "prisma:deploy:dev": "npm run prisma:generate:dev && npm run prisma:migrate:dev ",
    "prisma:generate:test": "cross-env NODE_ENV=test dotenv -e .env.test -- prisma generate",
    "prisma:migrate:test": "cross-env NODE_ENV=test dotenv -e .env.test -- prisma migrate dev",
    "prisma:deploy:test": "npm run prisma:generate:test && npm run prisma:migrate:test ",
    "prisma:deploy:all": "npm run prisma:deploy:dev && npm run prisma:deploy:test",
    "prisma:reset:dev": "cross-env NODE_ENV=development dotenv -e .env.development -- ts-node -r tsconfig-paths/register src/scripts/resetDb.ts",
    "prisma:reset:test": "cross-env NODE_ENV=test dotenv -e .env.test -- ts-node -r tsconfig-paths/register src/scripts/resetDb.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^5.20.0",
    "@types/multer": "^1.4.12",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nest-winston": "^1.9.7",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.20.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
